@prefix : <http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3/> .

<http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3> rdf:type owl:Ontology .

:Biocypherroot rdf:type owl:Class
        rdfs:label "Biocypherroot" .

#################################################################
#    Object Properties
#################################################################

:is_relative rdf:type owl:ObjectProperty ;
        rdfs:label "is_relative" ;
        rdfs:subClassOf :Biocypherroot ;
        rdfs:domain :Person ;
        rdfs:range :Person .

:has_partner rdf:type owl:ObjectProperty ;
        rdfs:label "has_partner" ;
        rdfs:subPropertyOf :is_relative ;
        rdfs:domain :Person ;
        rdfs:range :Person .

:child_of rdf:type owl:ObjectProperty ;
        rdfs:label "child_of" ;
        rdfs:subPropertyOf :is_relative ;
        owl:inverseOf :parent_of ;
        rdfs:domain :Person ;
        rdfs:range :Person .

:son_of rdf:type owl:ObjectProperty ;
        rdfs:label "son_of" ;
        rdfs:subPropertyOf :child_of ;
        owl:inverseOf :parent_of ;
        rdfs:domain :Male ;
        rdfs:range :Person .

:daugther_of rdf:type owl:ObjectProperty ;
        rdfs:label "daugther_of"
        rdfs:subPropertyOf :child_of ;
        owl:inverseOf :parent_of ;
        rdfs:domain :Female ;
        rdfs:range :Person .

:parent_of rdf:type owl:ObjectProperty ;
        rdfs:label "parent_of"
        rdfs:subPropertyOf :is_relative ;
        owl:inverseOf :child_of ;
        rdfs:domain :Person ;
        rdfs:range :Person .


#################################################################
#    Classes
#################################################################


:Father rdf:type owl:Class ;
        rdfs:label "Father" ;
        owl:equivalentClass [ owl:intersectionOf ( :Male
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :parent_of ;
                                                     owl:someValuesFrom :Person
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Parent .


:Female rdf:type owl:Class ;
        rdfs:label "Female" ;
        rdfs:subClassOf :Person .


:Male rdf:type owl:Class ;
      rdfs:label "Male" ;
      rdfs:subClassOf :Person .


:Mother rdf:type owl:Class ;
        rdfs:label "Mother" ;
        owl:equivalentClass [ owl:intersectionOf ( :Female
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :parent_of ;
                                                     owl:someValuesFrom :Person
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Parent .


:Parent rdf:type owl:Class ;
        rdfs:label "Parent" ;
        rdfs:subClassOf :Role .


:Person rdf:type owl:Class ;
        rdfs:label "Person" ;
        rdfs:subClassOf :Biocypherroot .


:Role rdf:type owl:Class ;
      rdfs:label "Role"
      rdfs:subClassOf :Biocypherroot .
