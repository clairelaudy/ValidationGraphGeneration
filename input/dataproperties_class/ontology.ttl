@prefix : <http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3/> .

<http://www.semanticweb.org/claudy/ontologies/2025/10/untitled-ontology-3> rdf:type owl:Ontology .

:Biocypherroot rdf:type owl:Class ;
                rdfs:label "Biocypherroot" .

:ObjectProperty rdf:type owl:Class ;
                rdfs:subClassOf :Biocypherroot ;
                rdfs:label "ObjectProperty" .

:DatatypeProperty rdf:type owl:Class ;
                rdfs:subClassOf :Biocypherroot ;
                rdfs:label "DatatypeProperty" .

#################################################################
#    Object Properties
#################################################################

:has_pet rdf:type owl:ObjectProperty ;
		 rdfs:subClassOf :ObjectProperty ;
         rdfs:label "has_pet" ;
         rdfs:domain :Person ;
         rdfs:range :Animal .


:child_of rdf:type owl:ObjectProperty ;
          rdfs:label "child_of" ;
          rdfs:subPropertyOf :is_relative ;
          owl:inverseOf :parent_of ;
          rdfs:domain :Person ;
          rdfs:range :Person .


:daugther_of rdf:type owl:ObjectProperty ;
             rdfs:label "daugther_of" ;
             rdfs:subPropertyOf :child_of ;
             owl:inverseOf :parent_of ;
             rdfs:domain :Female ;
             rdfs:range :Person .


:has_partner rdf:type owl:ObjectProperty ;
             rdfs:label "has_partner" ;
             rdfs:subPropertyOf :is_relative ;
             rdf:type owl:SymmetricProperty ;
             rdfs:domain :Person ;
             rdfs:range :Person .


:is_relative rdf:type owl:ObjectProperty ;
			 rdfs:subClassOf :ObjectProperty ;
             rdfs:label "is_relative" ;
             rdfs:domain :Person ;
             rdfs:range :Person .


:parent_of rdf:type owl:ObjectProperty ;
           rdfs:label "parent_of" ;
           rdfs:subPropertyOf :is_relative ;
           owl:inverseOf :son_of ;
           rdfs:domain :Person ;
           rdfs:range :Person .


:son_of rdf:type owl:ObjectProperty ;
        rdfs:label "son_of" ;
        rdfs:subPropertyOf :child_of ;
        rdfs:domain :Male ;
        rdfs:range :Person .


#################################################################
#    Data properties
#################################################################

:age rdf:type owl:DatatypeProperty ;
     rdfs:label "age" ;
     rdfs:domain :Animal ,
                 :Person ;
     rdfs:range xsd:int .


:first_name rdf:type owl:DatatypeProperty ;
      rdfs:label "first_name" ;
      rdfs:domain :Animal ,
                  :Person ;
      rdfs:range xsd:string .

:last_name rdf:type owl:DatatypeProperty ;
      rdfs:label "last_name" ;
      rdfs:domain :Animal ,
                  :Person ;
      rdfs:range xsd:string .


:possess_home rdf:type owl:DatatypeProperty ;
              rdfs:label "possess_home" ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Person ;
              rdfs:range xsd:boolean .


:rents_home rdf:type owl:DatatypeProperty ;
            rdfs:label "rents_home" ;
            rdfs:domain :Person ;
            rdfs:range xsd:boolean .


#################################################################
#    Classes
#################################################################

:Animal rdf:type owl:Class ;
        rdfs:label "Animal" ;
        rdfs:subClassOf :Biocypherroot .


:Cat rdf:type owl:Class ;
     rdfs:label "Cat" ;
     rdfs:subClassOf :Animal .


:Dog rdf:type owl:Class ;
     rdfs:label "Dog" ;
     rdfs:subClassOf :Animal .


:Parent rdf:type owl:Class ;
        rdfs:label "Parent" ;
        owl:equivalentClass [ owl:intersectionOf ( :Person
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :parent_of ;
                                                     owl:someValuesFrom :Person
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Person .


:Person rdf:type owl:Class ;
        rdfs:label "Person" ;
        rdfs:subClassOf :Biocypherroot .

